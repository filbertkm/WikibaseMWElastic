{
	"name": "mediawiki/wikibase-elastic",
	"type": "mediawiki-extension",
	"description": "MediaWiki extension that provides Wikibase integration with CirrusSearch",
	"keywords": [
		"wikibase",
		"wikidata"
	],
	"authors": [
		{
			"name": "Katie Filbert"
		}
	],
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/filbertkm/WikibaseElastic.git"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"wikibase/elastic": "dev-master"
	},
	"autoload": {
		"classmap": [
			"maintenance"
		],
		"psr-4": {
			"Wikibase\\Search\\Elastic\\": "src"
		}
	}
}
